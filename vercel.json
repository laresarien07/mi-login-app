{ 
  "version": 2, 
  "builds": [ 
    { 
      "src": "api/login.js", 
      "use": "@vercel/node" 
    }, 
    { 
      "src": "public/**", 
      "use": "@vercel/static" 
    } 
  ], 
  "routes": [ 
    { 
      "src": "/api/login", 
      "dest": "/api/login.js" 
    }, 
    { 
      "src": "/(.*)", 
      "dest": "/public/$1" 
    } 
  ] 
} 
